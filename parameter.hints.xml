<application>
  <component name="ParameterNameHintsSettings">
    <blacklists>
      <blacklist language="TypeScript" />
      <blacklist language="NBTT" />
      <blacklist language="Java" />
      <blacklist language="Groovy" />
      <blacklist language="Kotlin">
        <added pattern="com.nhaarman.mockito_kotlin.times(numInvocations)" />
        <added pattern="com.nhaarman.mockito_kotlin.eq(value)" />
        <added pattern="org.jetbrains.spek.api.dsl.it(description,body)" />
        <added pattern="org.jetbrains.spek.api.dsl.on(description,body)" />
        <added pattern="org.jetbrains.spek.api.dsl.context(description,body)" />
        <added pattern="org.gradle.api.plugins.ObjectConfigurationAction.plugin(pluginId)" />
        <added pattern="kotlin.collections.mapOf(pairs)" />
        <added pattern="kotlin.test.assertTrue(actual)" />
        <added pattern="kotlin.test.assertFalse(actual)" />
      </blacklist>
    </blacklists>
    <option id="SHOW_PROPERTY_HINT" value="true" />
    <option id="SHOW_FUNCTION_HINT" value="true" />
    <option id="SHOW_LOCAL_VARIABLE_HINT" value="true" />
    <option id="SHOW_PARAMETER_TYPE_HINT" value="true" />
  </component>
</application>